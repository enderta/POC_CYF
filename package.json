{
  "name": "POC_cyf",
  "main": "server.js",
  "type": "module",
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/step_definitions/*.steps.js",
    "nonGlobalStepDefinitions": true,
    "filterSpecs": true,
    "omitFiltered": true
  },
  "html": {
    "enabled": true
  },
  "json": {
    "enabled": true
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.1",
    "aws-sdk-client-mock": "^3.0.1",
    "aws-sdk-mock": "^5.9.0",
    "cypress": "^13.6.4",
    "cypress-mochawesome-reporter": "^3.8.1",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-real-events": "^1.11.0",
    "fauxauth": "^9.0.4",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "pdfkit": "^0.14.0",
    "react": "*",
    "react-dom": "*",
    "react-scripts": "*"
  },
  "scripts": {
    "report:merge": "mochawesome-merge cypress/results/*.json -o cypress/report.json",
    "report:generate": "marge cypress/report.json --charts true",
    "start": "node server.js",
    "dev": "concurrently -n \"fauxauth,some_app\" \"npm run fauxauth\" \"npm start\""
  },
  "engines": {
    "node": "^20.11.0"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.1",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "aws-sdk": "^2.1560.0",
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.12",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "dotenv": "^16.4.1",
    "express": "^4.17.1",
    "fs-extra": "^11.2.0",
    "morgan": "^1.10.0"
  }
}
