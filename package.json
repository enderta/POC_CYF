{
  "name": "POC_cyf",
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/step_definitions/*.steps.js",
    "nonGlobalStepDefinitions": true,
    "filterSpecs": true,
    "omitFiltered": true
  },
  "html": {
    "enabled": true
  },
  "json": {
    "enabled": true
  },
  "devDependencies": {
    "cypress": "^13.6.4",
    "cypress-mochawesome-reporter": "^3.8.1",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-real-events": "^1.11.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "pdfkit": "^0.14.0",
    "react": "*",
    "react-dom": "*",
    "react-scripts": "*"
  },
  "scripts": {
    "report:merge": "mochawesome-merge cypress/results/*.json -o cypress/report.json",
    "report:generate": "marge cypress/report.json --charts true"
  },
  "engines": {
    "node": "^20.11.0"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.1",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "dotenv": "^16.4.1",
    "fs-extra": "^11.2.0"
  }
}
